			<record-header-length>30</record-header-length> <!-- If the records header is present, this field specifies the length of the record header. -->
		</record-header>
		
		<file-header present="true"> <!-- Specifies whether the file header is present or not into the raw files. -->
			<file-header-parser>STANDARD</file-header-parser> <!-- BAF - This is Bellcore Format cdr in which header comes in format of xml. / STANDARD - This is standard header which comes with any seperator. -->
			<contains-fields>false</contains-fields> <!-- For standard format, it contains value or not. -->
		</file-header>
		
		<file-footer present="true"> <!-- Specifies whether the file footer is present or not into the raw files. -->
